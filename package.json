{
	"enableProposedApi": true,
	"name": "api-webserver",
	"displayName": "API_WEBSERVER",
	"publisher": "JannikSvensson",
	"description": "starts a webserver to which POST and GET can be sent to control visual studio code, ie. when developing a external code editor/generator using a standard webbrowser",
	"version": "1.0.3",
	"engines": {
		"vscode": "^1.50.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*",
		"onCommand:terminalTest.onDidWriteTerminalData"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "api-webserver.restart",
				"title": "API-webserver restart"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/express": "^4.17.8",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.50.0",
		"@types/ws": "^7.2.9",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"typescript": "^4.0.5",
		"glob": "^7.1.6",
		"mocha": "^8.1.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/manicken/vsc_api_webserver.git"
	},
	"dependencies": {
		"ansi-to-html": "^0.6.14",
		"express": "^4.17.1",
		"strip-ansi": "^6.0.0",
		"vscode-test": "^1.4.0",
		"ws": "^4.1.0"
	}
}
